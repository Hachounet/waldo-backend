{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": ["double"],
    "quotes": [],
    "max-len": [
      "error",
      {
        "code": 90
      }
    ]
  }
}
